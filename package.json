{
  "name": "lab-18",
  "version": "1.0.0",
  "description": "Socket.io",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint \"**/**.js\"",
    "test": "jest --verbose --coverage",
    "test-watch": "jest --watchAll --verbose --coverage",
    "jsdoc": "jsdoc -c ./docs/config/jsdoc.config.json .",
    "push-check": "npm test && npm run lint",
    "build-docs": "jsdoc -c ./docs/config/jsdoc.config.json . && open ./docs/index.html"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mwilkin-401-advanced-javascript/lab-18.git"
  },
  "author": "Matt Wilkin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mwilkin-401-advanced-javascript/lab-18/issues"
  },
  "homepage": "https://github.com/mwilkin-401-advanced-javascript/lab-18#readme",
  "dependencies": {
    "eslint": "^5.16.0",
    "jest": "^24.8.0",
    "jsdoc": "^3.6.2",
    "socket.io": "^2.2.0"
  }
}
